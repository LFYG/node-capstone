{{# def.utils }}
<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{= ctx.options.project || ctx.pkg.name }}</title>
    <base href="{{= ctx.base }}">
    <link href="bower_components/foundation/css/normalize.min.css" rel="stylesheet">
    <link href="bower_components/foundation/css/foundation.min.css" rel="stylesheet">
    <link href="bower_components/prism/themes/prism.css" rel="stylesheet">
    <link href="css/app.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">
    <script src="bower_components/modernizr/modernizr.js"></script>
  </head>
  <body>

    <nav class="top-bar" data-topbar>
      <ul class="title-area">
        <li class="name">
          <h1>
            <a href="index.html">{{= ctx.options.project || ctx.pkg.name }}</a>
          </h1>
        </li>
      </ul>
      <section class="top-bar-section">
        <ul class="right">
          <li class="active">
            <a href="{{= ctx.pkg.repository.url }}">
              <span class="fa fa-fw fa-2x fa-github-square"></span>Fork Me
            </a>
          </li>
        </ul>
      </section>
    </nav>

    <div class="row">
      <section class="sidebar small-3 column">
        {{# def.sidebar }}
      </section>
      <section class="content small-9 column">
